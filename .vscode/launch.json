    "version": "0.2.0",
    "configurations": [
        {
            "name": "varanasi_mobile_app",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_development.dart",
            "args": [
                "--flavor",
                "development",
            ]
        },
        {
            "name": "varanasi_mobile_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main_development.dart",
            "args": [
                "--flavor",
                "development"
            ]
        },
        {
            "name": "varanasi_mobile_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main_production.dart",
            "args": [
                "--flavor",
                "production"
            ]
        }
    ]
}