{
    "@@locale": "en",
    "appName": "{flavor, select, dev {Varanasi Dev} staging {Varanasi Staging} other {Varanasi}}",
    "@appName": {
        "description": "The name of the application",
        "type": "text",
        "placeholders": {
            "flavor": {
                "type": "String",
                "example": "Dev",
                "description": "The flavor of the application"
            }
        }
    },
    "greeting": "Hello, {name}!",
    "@greeting": {
        "description": "A greeting message",
        "type": "text",
        "placeholders": {
            "name": {
                "type": "String",
                "example": "World",
                "description": "The name of the person to greet"
            }
        }
    },
    "greetingSubtitle": "Let's find some music for you.",
    "@greetingSubtitle": {
        "description": "A subtitle for the greeting message",
        "type": "text"
    }
}